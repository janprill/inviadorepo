{
  "connection": "default",
  "collectionName": "organizations",
  "info": {
    "name": "organization"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "links": {
      "collection": "link",
      "via": "organization"
    },
    "isActive": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "members": {
      "via": "organizations",
      "collection": "member"
    }
  }
}
