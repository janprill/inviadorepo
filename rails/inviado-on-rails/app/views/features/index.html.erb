<p id="notice"><%= notice %></p>

<h1>Features</h1>

<table>
  <thead>
    <tr>
      <th>Key</th>
      <th>Source</th>
      <th>Version</th>
      <th>Period desc</th>
      <th>Period</th>
      <th>Title</th>
      <th>Description</th>
      <th>Raw</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @features.each do |feature| %>
      <tr>
        <td><%= feature.key %></td>
        <td><%= feature.source %></td>
        <td><%= feature.version %></td>
        <td><%= feature.period_desc %></td>
        <td><%= feature.period %></td>
        <td><%= feature.title %></td>
        <td><%= feature.description %></td>
        <td><%= feature.raw %></td>
        <td><%= link_to 'Show', feature %></td>
        <td><%= link_to 'Edit', edit_feature_path(feature) %></td>
        <td><%= link_to 'Destroy', feature, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Feature', new_feature_path %>
